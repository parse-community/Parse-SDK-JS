{
  "plugins": ["node_modules/jsdoc-babel", "plugins/markdown"],
  "babel": {
      "plugins": ["transform-flow-strip-types"]
  },
  "source": {
    "include":  ["./README.md"],
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "default": {
      "outputSourceFiles": false
    },
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "template": "node_modules/minami",
    "showInheritedInNav": true,
    "useLongnameInNav": true
  }
}
