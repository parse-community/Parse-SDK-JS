{
  "extends": "@definitelytyped/dtslint/dtslint.json",
  "rules": {
    "semicolon": false,
    "ban-types": false,
    "no-unnecessary-generics": false,
    "no-redundant-jsdoc": false,
    "strict-export-declare-modifiers": false,
    "interface-over-type-literal": false,
    "no-duplicate-imports": false,
    "array-type": false,
    "void-return": false
  }
}
